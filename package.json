{
  "name": "@europeana/portal",
  "version": "1.47.0",
  "private": true,
  "description": "Europeana Portal",
  "type": "module",
  "scripts": {
    "api:start": "node --experimental-json-modules bin/api.js",
    "build": "nuxt build",
    "test:unit": "vue-cli-service test:unit --include tests/unit/setup",
    "test:e2e": "touch tests/e2e/docker/nightwatch-visual/.env && docker-compose -f ./tests/e2e/docker/docker-compose.yml build -q nightwatch-features && docker-compose -f ./tests/e2e/docker/docker-compose.yml run --rm nightwatch-features",
    "lint": "vue-cli-service lint bin src tests/unit *.js",
    "analyze": "nuxt build --analyze",
    "build:test": "cross-env NODE_ENV=test nuxt build",
    "cache": "node --experimental-json-modules --require dotenv/config src/cachers/index.js",
    "dev": "node --experimental-json-modules node_modules/.bin/nuxt",
    "generate": "nuxt generate",
    "lint:fix": "npm run lint -- --fix",
    "start": "node --experimental-json-modules node_modules/.bin/nuxt start",
    "start:inspect": "node --inspect node_modules/.bin/nuxt start",
    "stylelint": "stylelint \"**/*.scss\" \"**/*.vue\" --config .stylelintrc.cjs",
    "test": "npm run test:unit && npm run test:size && npm run test:e2e",
    "test:size": "npm run build && cd tests/size/ && npm install && npm run start",
    "test:stack:build": "docker-compose -f ./tests/e2e/docker/docker-compose.yml build",
    "test:stack:down": "docker-compose -f ./tests/e2e/docker/docker-compose.yml down",
    "test:stack:rebuild": "docker-compose -f ./tests/e2e/docker/docker-compose.yml down && npm run test:stack:build",
    "test:stack:start": "docker-compose -f ./tests/e2e/docker/docker-compose.yml start app nginx chrome-en chrome-ja chrome-nl",
    "test:stack:stop": "docker-compose -f ./tests/e2e/docker/docker-compose.yml stop app nginx chrome-en chrome-ja chrome-nl",
    "test:visual": "docker-compose -f ./tests/e2e/docker/docker-compose.yml build -q nightwatch-visual && docker-compose -f ./tests/e2e/docker/docker-compose.yml run --rm nightwatch-visual",
    "version": "node --experimental-json-modules bin/version.js && git add .sonarcloud.properties"
  },
  "dependencies": {
    "@elastic/apm-rum-vue": "^1.1.9",
    "@nuxtjs/auth": "^4.9.1",
    "@nuxtjs/axios": "^5.13.1",
    "@nuxtjs/robots": "^2.4.2",
    "@vue-a11y/announcer": "^2.1.0",
    "autosuggest-highlight": "^3.1.1",
    "axios": "^0.21.1",
    "bootstrap": "^4.6.0",
    "bootstrap-vue": "^2.21.2",
    "contentful": "^8.4.2",
    "cookie-universal-nuxt": "^2.0.18",
    "core-js": "^3.15.1",
    "dateformat": "^4.5.1",
    "deep-object-diff": "^1.1.0",
    "deepmerge": "^4.2.2",
    "defu": "^5.0.0",
    "dotenv": "^10.0.0",
    "elastic-apm-node": "^3.9.0",
    "express": "^4.17.1",
    "lodash": "^4.17.21",
    "marked": "^0.7.0",
    "md5": "^2.3.0",
    "morgan": "^1.9.1",
    "nuxt": "^2.15.7",
    "nuxt-i18n": "^6.10.1",
    "qs": "^6.6.0",
    "redis": "^3.1.2",
    "swiper": "^6.1.2",
    "vue": "^2.6.14",
    "vue-awesome-swiper": "^4.1.1",
    "vue-client-only": "^2.0.0",
    "vue-i18n": "^8.17.3",
    "vue-masonry": "^0.12.0",
    "vue-matomo": "^4.1.0",
    "vue-scrollto": "^2.18.2",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@babel/runtime": "^7.14.6",
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-plugin-eslint": "^4.5.13",
    "@vue/cli-plugin-unit-mocha": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "@vue/test-utils": "^1.2.1",
    "camelcase": "^6.0.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-string": "^1.5.0",
    "contentful-cli": "^1.5.32",
    "cross-env": "^5.2.1",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^6.2.2",
    "eslint-webpack-plugin": "^2.5.4",
    "nock": "^10.0.6",
    "node-sass": "^6.0.1",
    "properties-reader": "^2.2.0",
    "sass-loader": "^10.0.0",
    "sinon": "^7.2.7",
    "sinon-chai": "^3.3.0",
    "style-resources-loader": "^1.4.1",
    "stylelint": "^13.7.1",
    "stylelint-config-recommended": "^3.0.0",
    "svg-url-loader": "^7.1.1",
    "vue-eslint-parser": "^7.8.0"
  },
  "contributors": [
    "Richard Doe <richard.doe@europeana.eu>",
